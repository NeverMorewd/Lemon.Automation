{
  "manifest_version": 3,
  "name": "lemon",
  "version": "0.0.1",
  "description": "just for test",
  "background": {
    "service_worker": "iirpa/service_worker.js"
  },
  "options_ui": {
    "page": "src/options.html",
    "open_in_tab": true
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "iirpa/content.js"
      ],
      "match_about_blank": true,
      "matches": [
        "http://*/*",
        "https://*/*",
        "ftp://*/*",
        "file://*/*"
      ],
      "run_at": "document_start"
    }
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self'; font-src 'self' https://fonts.gstatic.com/ https://res-1.cdn.office.net/;"
  },
  "permissions": [
    "storage",
    "cookies",
    "management",
    "tabs",
    "debugger",
    "nativeMessaging",
    "webNavigation",
    "downloads",
    "clipboardRead",
    "clipboardWrite",
    "scripting",
    "contextMenus",
    "activeTab"
  ],
  "host_permissions": [
    "\u003Call_urls>"
  ],
  "icons": {
    "16": "icons/设计器-16x16.png",
    "32": "icons/设计器-32x32.png",
    "48": "icons/设计器-48x48.png",
    "128": "icons/设计器-128x128.png"
  }
}
